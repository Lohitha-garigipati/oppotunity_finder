    <div id="main_div">
  <form action="/user/updateDetails" method="POST">
  
    <div class="form-row">
      <div class="form-group col-md-6">
        <label for="fname">1. Client Name</label>
      
        <input type="text-center"  class="form-control" id="cname" maxlength="30" name="clientName" value="{{process.clientName}}" readonly>
      </div>
    <div class="form-group col-md-6">
        <label for="lname">2. Business Unit</label>
      
        <input type="text-center" class="form-control" id="busiunit" maxlength="30" name="Buss_Unit" value="{{process.Buss_Unit}}" readonly>
      </div>
    </div>
    <div class="form-row">
      <div class="form-group col-md-6">
        <label for="country">3. Sub Business Unit</label>  
      
        <input type="text-center" class="form-control" id="subbunit" maxlength="30" name="Sub_Buss_Unit" value="{{process.Sub_Buss_Unit}}" readonly>
      </div>
    <div class="form-group col-md-6">
          <label for="country">4. Process Name</label>  
        
          <input type="text-center" class="form-control" id="pname" maxlength="30" name="Proc_Name" value="{{process.Proc_Name}}" readonly>
        </div>
      </div>
      <div class="form-row">
        <div class="form-group col-md-6">
          <label for="country">5. Process Id</label>  
        
          <input type="text-center"  class="form-control" id="procid" maxlength="30" name="Proc_Id" value="{{process.Proc_Id}}" readonly>
        </div>
      <div class="form-group col-md-6">
        <label for="subject">6. Process Description</label>
      
        <input id="subject" class="form-control"  name="Proc_Desc" style="height:120px" value="{{process.Proc_Desc}}" readonly>
      </div>
    </div>
    <div class="form-row">
        <div class="form-group col-md-6">
          <label for="country">7. Monthly Volume</label>  
        
            <input type="number-center" class="form-control" oninput="calculate()"   id="mv" maxlength="30" name="Mon_Vol" value="{{process.Mon_Vol}}"readonly/>
        </div>
      <div class="form-group col-md-6">
          <label for="country">8. AHT(in minutes)</label>  
        
            <input type="number-center" class="form-control" oninput="calculate()"   id="aht" step="any" maxlength="30" name="AHT" value="{{process.AHT}}" readonly/>
        </div>
      </div>
      <div class="form-row">
       <div class="form-group col-md-6">
          <label for="country">9. FTE</label>  
       
            <input type="number-center"   class="form-control"  id="fte" step="any" maxlength="30" name="FTE" value="{{process.FTE}}" readonly>
        </div>
      <div class="form-group col-md-6">
          <label for="sla">10. SLA</label>
        
          <select  class="form-control" id="slacalc" name="SLA">
                <option>{{process.SLA}}</option>
                {{!-- <option readonly>{{process.SLA}}</option> --}}
                <option id="1" value="Less than 8 hrs" disabled>Less than 8 hrs</option>
                     <option id="2" value="Less than 12 hrs" disabled>Less than 12 hrs</option>
                     <option id="3" value="Less than 24 hrs" disabled>Less than 24 hrs</option>
                     <option id="4" value="Less than 48 hrs" disabled>Less than 48 hrs</option>
                
          </select>
        </div>
      </div>
      
    
    
    
    <div class="form-row">
        <div class="form-group col-md-6">
          <label for="fname">11. TAT</label>
        
          <select  class="form-control" id="TAT" name="TAT">
                
                <option> {{process.TAT}} </option>
                <option id="5" value="Less than 8 hrs" disabled>Less than 8 hrs</option>
                     <option id="6" value="Less than 12 hrs" disabled>Less than 12 hrs</option>
                     <option id="7" value="Less than 24 hrs" disabled>Less than 24 hrs</option>
                     <option id="8" value="Less than 48 hrs" disabled>Less than 48 hrs</option>
                
                     </select>
        </div>
      <div class="form-group col-md-6">
            <label for="lname">12. Applications Used</label>
          
            <input  class="form-control" type="text-center" id="appused" maxlength="150" name="App_Used" value="{{process.App_Used}}" readonly>
          </div>
        </div>
        <div class="form-row">
            <div class="form-group col-md-6">
              <label for="country">13. Documentations of the process</label>  
            
                  <select  class="form-control" id="docpre" name="Doc_Present">
                          
                          <option> {{process.Doc_Present}} </option>
                           <option id="9" value="No Documenation" disabled>No Documenation</option>
                           <option id="10" value="Partial Documentation" disabled>Partial Documentation</option>
                           <option id="11" value="Detailed Documentation" disabled>Detailed Documentation</option>
                          </select>
            </div>
         <div class="form-group col-md-6">
                <label for="country">14. Rule based process</label>  
              
                      <select class="form-control"  id="rbased" name="Rule_Based">
                             
                             <option>{{process.Rule_Based}} </option>
                              <option id="12" value="High" disabled>High</option>
                              <option id="13" value="Medium" disabled>Medium</option>
                              <option id="14" value="Low" disabled>Low</option>

                             </select>
              </div>
            </div>
          <div class="form-row">
            <div class="form-group col-md-6">
              <label for="subject">15. Structure of Input Data</label>
            
                  <select class="form-control" id="dattype" name="Stuc_Data">
                          
                          <option>{{process.Stuc_Data}}</option>
                      <option id="15" value="Structured" disabled>Structured</option>
                      <option id="16" value="Unstructured" disabled>Unstructured</option>
                      <option id="17" value="Mix of both" disabled>Mix of both</option>
                     </select>
            </div>
          <div class="form-group col-md-6">
                <label for="country">16. Input data type</label>  
             
                      <select  class="form-control" id="indat" name="Inp_Data_Type">
                               
                               <option>{{process.Inp_Data_Type}}</option>
                          <option id="18" value="Digitized" disabled>Digitized</option>
                          <option id="19" value="Non Digitized" disabled>Non Digitized</option>
                          <option id="20" value="Mix of Both" disabled>Mix of both</option>

                         </select>
              </div>
            </div>
            <div class="form-row">
              <div class="form-group col-md-6">
                <label for="country">17. Percentage of process amenable for RPA</label>  
              
                      <input class="form-control" id="arpa" type="number-center" step="any" name="Amenable_RPA" value="{{process.Amenable_RPA}}" readonly>
              </div>
            <div class="form-group col-md-6">
                <label for="country">18. Percentage of process amenable for cognitive</label>  
              
                      <input class="form-control" id="ac" type="number-center" step="any" name="Amenable_Cognitive" value="{{process.Amenable_Cognitive}}" readonly>
              </div>
            </div>
            <div class="form-row">
              <div class="form-group col-md-6">
                <label for="sla">19. Automation Readiness</label>
              
                      <input class="form-control" type="number-center" step="any" name="Automation_Ready" value="{{process.Automation_Ready}}" readonly>
              </div>
            <div class="form-group col-md-6">
                <label for="sla">20. Percentage of Automation Potential</label>
              
                      <input class="form-control" type="number-center" step="any" name="AP_Perc" value="{{process.AP_Perc}}" readonly>
              </div>
            </div>
            {{!-- <div class="form-row"> --}}
              <div class="form-group col-md-6">
                <label for="sla">21. FTE Benifit</label>
              
                      <input class="form-control" id="fteb" type="number-center" step="any" name="FTE_Benefit" value="{{process.FTE_Benefit}}" readonly>
              </div>
           
           
          
          
          
          
          

